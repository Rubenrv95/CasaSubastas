package casasubasta;

import java.util.ArrayList;

/**
 *
 * @author Rubén
 */
public class Artículo {
    int precio;
    String nombre;
    ArrayList<String> pujas;

    public Artículo(int precio, String nombre, ArrayList<String> pujas) {
        this.precio = precio;
        this.nombre = nombre;
        this.pujas = pujas;
    }

    
    

    public int getPrecio() {
        return precio;
    }

    public void setPrecio(int precio) {
        this.precio = precio;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public ArrayList<String> getPujas() {
        return pujas;
    }

    public void setPujas(ArrayList<String> pujas) {
        this.pujas = pujas;
    }
    
    
    
}
