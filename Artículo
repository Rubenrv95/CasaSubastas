
package casasubasta;

/**
 *
 * @author Rubén
 */
public class Artículo {
    int precio;
    String nombre;
    int pujas;

    public Artículo(int precio, String nombre, int pujas) {
        this.precio = precio;
        this.nombre = nombre;
        this.pujas = pujas;
    }

    public int getPrecio() {
        return precio;
    }

    public void setPrecio(int precio) {
        this.precio = precio;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
    
    
    
}
